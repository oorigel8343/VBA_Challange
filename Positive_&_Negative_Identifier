Sub Poitive_and_Negative_Change_Identifier():

Dim Summary_Table As Integer

Summary_Table = 2

For I = 2 To 705714

If Cells(I, 10).Value > 0 Then

Cells(I, 10).Interior.ColorIndex = 4

ElseIf Cells(I, 10).Value < 0 Then

Cells(I, 10).Interior.ColorIndex = 3

End If

Next I

End Sub